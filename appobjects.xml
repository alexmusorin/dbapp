<object name="App" type="inline" objclass="">
    <propertys>
	    <property name="WorkDir" type="String">
			<desc>
				<![CDATA[Каталог из которого запущено приложение]]> 
			</desc>
			<sample>
			  <![CDATA[MsgBox App.WorkDir]]>
			</sample>
		</property>
		<property name="DBTables" type="Collection">
		     <desc>
				<![CDATA[]]> 
			</desc>
			<sample>
			  <![CDATA[Debug.ClearText 
'For i = 0 to App.DBTables.Count - 1 
For Each Table in App.DBTables
  With Table  'App.DBTables(i) 
    Debug.Print "Таблица:", .Name, VbCrLf, "===СПИСОК ПОЛЕЙ ТАБЛИЦЫ=======" 
    .Open
    'For j = 0 to .Fields.Count - 1 
    '   Debug.Print vbTab, .Fields(j).FieldName, ":", .Fields(j).DataType
    'Next
    For Each Field In .Fields 
        Debug.Print vbTab, Field.FieldName, ":", Field.DataType
    Next
    .Close
  End With
Next]]>
			</sample>
		</property>	
	</propertys>
